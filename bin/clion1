#!/bin/bash
#Generated by JetBrains Toolbox 2.1.3.18901 at 2023-12-22T23:24:52.918172

declare -a intellij_args=()
declare -- wait=""

for o in "$@"; do
  if [[ "$o" = "--wait" || "$o" = "-w" ]]; then
    wait="-W"
    o="--wait"
  fi
  if [[ "$o" =~ " " ]]; then
    intellij_args+=("\"$o\"")
  else
    intellij_args+=("$o")
  fi
done

open -na "/Users/hijiri/Applications/CLion Nova.app/Contents/MacOS/clion" $wait --args "${intellij_args[@]}"